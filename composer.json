{
    "name": "elshaden/popup-card",
    "description": "A customizable popup modal card for Laravel Nova that automatically displays to users when they access your dashboard or resource pages.",
    "keywords": [
        "laravel",
        "nova",
        "popup",
        "modal",
        "notification",
        "announcement",
        "card",
        "laravel-nova"
    ],
    "homepage": "https://github.com/elshaden/popup-card",
    "support": {
        "issues": "https://github.com/elshaden/popup-card/issues",
        "source": "https://github.com/elshaden/popup-card"
    },
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "laravel/nova": "^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Elshaden\\PopupCard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elshaden\\PopupCard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elshaden\\PopupCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
